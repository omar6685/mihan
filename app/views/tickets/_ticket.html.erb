<div id="<%= dom_id ticket %>">
  <p>
    <strong>Title:</strong>
    <%= ticket.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= ticket.description %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= ticket.status %>
  </p>

  <p>
    <strong>Chat history:</strong>
    <%= ticket.chat_history %>
  </p>

  <p>
    <strong>User:</strong>
    <%= ticket.user_id %>
  </p>

</div>
